{
    "manifest_version": 3,
    "name": "JSTAR Tab",
    "version": "1.0",
    "description": "A sleek modern custom new tab with search, shortcuts, and settings.",
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "permissions": ["storage"],
    "action": {
        "default_title": "New JSTAR Tab"
    }
}